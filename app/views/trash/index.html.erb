<h3> Trash: </h3>

<% if !@cars.empty? %>
    <div class="d-flex align-content-center flex-wrap ">
        <% @cars.each do |car| %>
            <%= render 'shared/card_form', object: car, btn: 'cars' %>
        <% end %>
    </div>
<% end %>

<% if !@races.empty? %>
    <div class="d-flex align-content-center flex-wrap ">
        <% @races.each do |race| %>
            <%= render 'shared/card_form', object: race, btn: 'races'%>
        <% end %>
    </div>
<% end %>


